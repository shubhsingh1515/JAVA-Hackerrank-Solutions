
import java.io.*;
import java.util.*;
import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str=sc.next();
        String rev = "";
        for(int i=str.length()-1;i>=0;i--)
            rev+=str.charAt(i);
        if(rev.equals(str))
            System.out.println("Yes");
        else
                    System.out.println("No");

        
        
    }
}




